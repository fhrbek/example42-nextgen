{
  "metaVersion": "1.1.4",
  "puppetVersion": "PUPPET27",
  "environments": [
    {
      "siteLocation": "manifests",
      "moduleLocations": [
        "modules"
      ],
      "name": "production"
    }
  ],
  "tests": [
    {
      "rakeFile": "rakewrappers/apache.rake",
      "rakeTask": "default",
      "outputs": [
        {
          "location": "modules/apache/spec/reports",
          "name": "Apache Module Test Results"
        }
      ],
      "name": "apache"
    }
  ]
}